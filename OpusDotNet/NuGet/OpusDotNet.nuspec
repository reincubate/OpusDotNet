<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>OpusDotNet</id>
    <version>1.0.3-RI.1</version>
    <description>An easy-to-use library for encoding and decoding 16-bit audio using the Opus Audio Codec. NOTE: This library is a wrapper around the official libopus, and does not contain code for parsing .opus container files.</description>
    <authors>mrphil2105</authors>
    <license type="expression">MIT</license>
    <projectUrl>https://github.com/mrphil2105/OpusDotNet</projectUrl>
    <copyright>Copyright © 2019 mrphil2105</copyright>
    <tags>Opus, Audio, Codec, Encoder, Decoder, VoIP</tags>

    <releaseNotes>Added improved encode and decode methods. IMPORTANT: Deprecated the old encode and decode methods.</releaseNotes>
  </metadata>
  <files>
    <file src="..\bin\Release\netstandard2.0\*.*" target="lib\netstandard2.0" />
    
    <file src="opus\LICENSE.txt" target="" />
    <file src="opus\win-x86\opus.dll" target="runtimes\win-x86\native" />
    <file src="opus\win-x64\opus.dll" target="runtimes\win-x64\native" />
    <file src="opus\win-ARM64\opus.dll" target="runtimes\win-ARM64\native" />

    <file src="OpusDotNet.targets" target="build" />
    <file src="OpusDotNet.props" target="build" />
  </files>
</package>
